<div id="<%= dom_id record %>">
  <p>
    <strong>Titulo:</strong>
    <%= record.title %>
  </p>
  <p>
    <strong>Monto:</strong>
    <%= record.amount %>
  </p>
  <p>
    <strong>Tipo:</strong>
    <% if record.category.is_income %>
      <%= "Ingreso" %>
    <% else %>
      <%= "Egreso" %>
    <% end %>
  </p>

</div>